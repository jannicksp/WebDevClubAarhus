@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Master>
	@using ContentModels = Umbraco.Web.PublishedModels;
	@{
	Layout = null;
	}
	
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="~/Content/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/Site.css" />
    <script src="~/Scripts/jquery-3.5.1.min.js"></script>
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
</head>

<body>
    @Umbraco.RenderMacro("Navigation")
    <main class="mt-3 ml-3 mr-3 mb-3">
        
        @RenderBody()
    
    </main>
   <!-- Footer -->
<footer class="bg-dark text-center text-lg-start" style="color:wheat;">
  <!-- Grid container -->
  <div class="container p-4">
    <!--Grid row-->
    <div class="row">
      <!--Grid column-->
      <div class="col-md-12 mb-4 mb-md-0">
    </div>
    <!--Grid row-->
  </div>
  <!-- Grid container -->

  <!-- Copyright -->
  <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2)">
    © 2020 Copyright:
    
    <a class="text-light" href="https://jannicksp.dk">@Model.Value("footer", fallback: Fallback.ToAncestors)</a>
  </div>
  <!-- Copyright -->
</footer>
<!-- Footer -->

</body>